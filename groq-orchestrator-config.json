{
  "name": "Consciousness Revolution GroqAI Orchestrator",
  "version": "1.0.0",
  "description": "Repository-wide AI orchestration using GroqAI as the primary provider",
  "provider": "groq",
  "apiKey": "",
  "apiKeyEnv": "GROQ_API_KEY",
  "enabled": true,
  "priority": 1,
  "freeTier": true,
  "maxRequestsPerDay": 14400,
  "models": {
    "chat": {
      "primary": "llama-3.3-70b-versatile",
      "fast": "llama-3.1-8b-instant",
      "alternatives": [
        "llama-3.1-8b-instant",
        "mixtral-8x7b-32768",
        "gemma2-9b-it"
      ]
    },
    "audio": {
      "transcription": "whisper-large-v3"
    }
  },
  "features": {
    "autoRetry": true,
    "autoFallback": true,
    "rateLimiting": true,
    "errorHandling": "exponential-backoff",
    "logging": true
  },
  "integrations": {
    "arayaSystem": true,
    "cyclotronBrain": true,
    "trinityOrchestration": true,
    "patternDetectors": true
  },
  "fallbackProviders": [
    "openai",
    "huggingface"
  ],
  "endpoints": {
    "baseUrl": "https://api.groq.com/openai/v1",
    "chat": "/chat/completions",
    "audio": "/audio/transcriptions"
  },
  "usage": {
    "projects": [
      "araya-consciousness-system",
      "cyclotron-brain",
      "pattern-detection-tools",
      "consciousness-dashboard",
      "seven-domains-assessment",
      "all-ai-dependent-features"
    ]
  },
  "monitoring": {
    "healthChecks": true,
    "requestTracking": true,
    "errorReporting": true
  },
  "security": {
    "keyRotation": false,
    "encryptAtRest": false,
    "maskInLogs": true
  }
}
